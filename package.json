{
  "name": "recompose-utils",
  "version": "1.0.3",
  "description": "Utilities for recompose",
  "keywords": [
    "react",
    "reactjs",
    "recompose"
  ],
  "main": "./src/index.js",
  "scripts": {
    "build": "npm run lint && babel --presets=es2015 ./src -d ./build --presets=es2015 --copy-files --ignore ./src/templates/modules ",
    "lint": "eslint ./src",
    "lint-fix": "eslint ./src --fix",
    "test": "nyc mocha --require babel-core/register",
    "lcov": "http-server ./coverage/lcov-report/",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "author": {
    "name": "Shalva Gegia",
    "email": "shalva.gegia@gmail.com",
    "url": "http://gegia.me/"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/shakogegia/recompose-utils"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "recompose": "^0.27.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
